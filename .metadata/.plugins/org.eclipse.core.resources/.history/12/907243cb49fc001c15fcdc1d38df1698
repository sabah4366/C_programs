#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,k=5,j;
	for(i=5;i>=1;i--){
		for(j=5;j>=1;j--)
		{
			printf("%d",j);
		}
		printf("\n");
	}



	return EXIT_SUCCESS;
}
