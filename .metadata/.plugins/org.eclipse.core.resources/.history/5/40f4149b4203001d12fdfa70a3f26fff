#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,j,limit=5;
	for(i=limit;i>=1;i--)
	{printf("\n");
		for(j=i;j<=5;j++)
		{
			printf("%c",j+64);
		}
	}

	return EXIT_SUCCESS;
}
