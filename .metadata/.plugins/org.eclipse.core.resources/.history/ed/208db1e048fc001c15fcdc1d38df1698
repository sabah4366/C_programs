#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,k=5,j;
	for(i=1;i<=5;i++){
		for(j=5;j>=i;j--{
			printf("%c",j+64);
		}
		printf("\n");
	}



	return EXIT_SUCCESS;
}
