#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,j,w,l;
	printf("enter the no of width");
	scanf("%d",&l);
	printf("enter the number of length");
	scanf("%d",&w);
	for(i=1;i<=w;i++){
		for(j=1;j<=l;j++)
		{
			if(i==1||i==l||j==1||j==w)
			{
				printf("*");
			}
			printf("\n");
		}
	}
	return EXIT_SUCCESS;
}
